{
  "name": "ayajs",
  "version": "2.0.7",
  "description": "A flexible JavaScript library for building any kind of diagrams quickly and in a programmatic way",
  "module": "build/aya.module.js",
  "type": "module",
  "main": "build/aya.js",
  "scripts": {
    "start": "rollup -c rollup.dev.js",
    "build": "rollup -c && npm run uglify",
    "uglify": "uglifyjs --v8 --toplevel --compress --mangle --verbose  -m reserved=['$','require','exports'] --output build/aya.min.js build/aya.js"
  },
  "keywords": [
    "diagram",
    "aya",
    "ayajs"
  ],
  "bug":{
    "url": "https://github.com/Duamelo/ayajs/issues"
  },
  "homepage": "https://ayajs.com",
  "author": "emack.io",
  "license": "ISC",
  "dependencies": {
  }
}
